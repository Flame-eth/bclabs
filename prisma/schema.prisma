datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Asset {
  id             Int      @id @default(autoincrement())
  symbol         String  @unique
  price          Float
  image          String
  name           String
  percentage_24H Float
  price_24H      Float
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
